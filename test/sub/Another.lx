decor AndMore:
	def onExpr(e: expr) -> expr:
		return expr.fromString(e.toString() + " + ' and more!'");

enum Color:
	case Red;
	case Green(str);
	case Blue(int, int);

	def toString:
		return switch this:
			case Red: "red";
			case Green(_): "green";
			case Blue(_, _): "blue";
