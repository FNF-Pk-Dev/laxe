decor AndMore:
	def onExpr(e: `expr) -> `expr:
		return `expr.fromString(e.toString() + " + ' and more!'");

enum Color:
	Red;
	Green(str);
	Blue(int, int);

	def toString:
		return switch this:
			case Red: "red";
			case Green(_): "green";
			case Blue(_, _): "blue";
