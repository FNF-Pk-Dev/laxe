def helloFromLaxe():
	trace("Hello from Laxe!");

	const a = Blue(123, 123) as Color;
	switch a:
		case Red: trace("is red");
		case Green(s): trace("is green: " + s);
		case _: trace("is something else");
	
	trace(a.toString());

